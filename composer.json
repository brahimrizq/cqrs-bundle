{
  "name": "nixilla/cqrs-bundle",
  "type": "symfony-bundle",
  "description": "Symfony Bundle that integrates Prooph CQRS/ES library",
  "keywords": [
    "cqrs",
    "es"
  ],
  "homepage": "https://github.com/nixilla/cqrs-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Janusz Slota",
      "email": "janusz.slota@nixilla.com"
    }
  ],
  "require": {
    "php": ">=5.6",
    "symfony/framework-bundle": "~2.3|~3.0"
  },
  "require-dev": {
    "phpspec/phpspec": "^3",
    "satooshi/php-coveralls": "^1.0",
    "henrikbjorn/phpspec-code-coverage": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Nixilla\\CqrsBundle\\": "src/"
    }
  },
  "config": {
    "bin-dir": "bin"
  }
}